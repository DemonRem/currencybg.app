<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">

    <TextView
        android:id="@+id/thirdparty_title"
        style="@style/FontStyle3rdTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="14sp"
        android:text="@string/about_3rdparty_icons"/>

    <TextView
        android:id="@+id/license_icons"
        style="@style/FontStyle3rdLibContent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="14sp"
        android:text="@string/license_icons"/>

</LinearLayout>
